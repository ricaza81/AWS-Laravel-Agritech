    $(document).ready(function() {
  var tour = new Tour({
     backdrop: true,
  steps: [
  {
    element: "#collapseOne1",
    title: "Estos son tus datos de contacto",
    content: "Información importante para contactos claves"
  },
    {
    element: "#collapseTwo2",
    title: "Aquí puedes agregar o administrar",
    content: "Una nueva oferta o editar una existente"
  },
      {
    element: "#collapseContact",
    title: "Aquí encuentras",
    content: "Los contactos recibidos y solicitudes que has enviado"
  },
    {
    element: "#news",
    title: "Cada vez que publicas una nueva oferta",
    content: "Se muestra en este bloque"
  },
      {
    element: "#contacto",
    title: "Contacto Directo",
    content: "Podrás ponerte en contacto directo con el agricultor, al presionar este botón"
  },
        {
    element: "#edit_user",
    title: "Al hacer clic sobre tu nombre",
    content: "Podrás editar tus datos"
   // path: "localhost/directo/public/sidebar"
  },
        {
    element: "#shop",
    title: "Plaza de Mercado",
    content: "Al hacer clic en este botón, podrás ver los productos en venta"
  }

]});

// Initialize the tour
tour.init();

// Start the tour
tour.start();
//tour.restart();
  });
</script>

